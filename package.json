{
  "name": "reporting",
  "version": "1.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --port 4400",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:timbrage": "ng serve --open --port 4402 --project timbrage",
    "build:timbrage": "ng build --prod --project timbrage",
    "deploy:timbrage": "firebase deploy --only hosting:timbrage",
    "predeploy:timbrage": "npm run build:timbrage",
    "http:timbrage": "npm run build:timbrage && cd dist/timbrage && angular-http-server --open -p 4402",
    "docker:build:timbrage": "docker build -t ifocusit/reporting/timbrage --build-arg target=timbrage .",
    "docker:run:timbrage": "docker run -p 4402:80 ifocusit/reporting/timbrage",
    "docker:upload:timbrage": "docker save ifocusit/reporting/timbrage:latest | ssh -C remote docker load",
    "start:admin": "ng serve --open --port 4401 --project admin",
    "build:admin": "ng build --prod --project admin",
    "deploy:admin": "firebase deploy --only hosting:admin",
    "predeploy:admin": "npm run build:admin",
    "http:admin": "npm run build:admin && cd dist/admin && angular-http-server --open -p 4401",
    "docker:build:admin": "docker build -t ifocusit/reporting/admin --build-arg target=admin .",
    "docker:run:admin": "docker run -p 4401:80 ifocusit/reporting/admin",
    "docker:upload:admin": "docker save ifocusit/reporting/admin:latest | ssh -C remote docker load"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/cdk": "~8.1.2",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/fire": "^5.2.1",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "~8.0.0",
    "@angular/material": "^8.1.2",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/pwa": "^0.801.3",
    "@angular/router": "~8.0.0",
    "@angular/service-worker": "~8.0.0",
    "@ngxs/store": "^3.5.0",
    "firebase": "^6.3.3",
    "firebaseui": "^4.1.0",
    "firebaseui-angular": "^4.0.0",
    "hammerjs": "^2.0.8",
    "help": "^3.0.2",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular-devkit/build-ng-packagr": "~0.800.6",
    "@angular/cli": "~8.0.2",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@ngxs/devtools-plugin": "^3.5.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.1.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
}